<% 
   require "log_common"
   require "platform"
   require "ui_utils"

   local filename = log_dir .. "/cmdsrv.log"
   content = read_file(filename)
   if not content then
      content = "ERROR: Could not open " .. tostring(filename)
   end
   print_log_page_content("cmdsrvlog", content)
%>
